# Each letter represents a colour marker:
#   k, r, g, y, b, m, c, w, d
# which stands for:
#   blacK, Red, Green, Yellow, Blue, Magenta, Cyan, White, Default
# Capital letters represent bold

[syntax]
keyword = y
name = c
comment = b
string = m
error = r
number = G
operator = Y
punctuation = y
token = C
paren = R

[interface]
background = k
output = w
main = c
prompt = c
prompt_more = g
